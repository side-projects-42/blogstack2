n  \"items\": [\n    { \"id\": 1, \"name\": \"Apples\",  \"price\": \"$2\" },\n    { \"id\": 2, \"name\": \"Peaches\", \"price\": \"$5\" }\n  ]\n}\n"}],"position":{"start":{"line":25,"column":1,"offset":1196},"end":{"line":33,"column":4,"offset":1330}}}],"position":{"start":{"line":25,"column":1,"offset":1196},"end":{"line":33,"column":4,"offset":1330}}},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-js"]},"children":[{"type":"text","value":"//\nclass MyComponent extends React.Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            error: null,\n            isLoaded: false,\n            items: []\n        };\n    }\n\n    componentDidMount() {\n        fetch('https://api.example.com/items')\n            .then((res) => res.json())\n            .then(\n                (result) => {\n                    this.setState({\n                        isLoaded: true,\n                        items: result.items\n                    });\n                },\n\n                (error) => {\n                    this.setState({\n                        isLoaded: true,\n                        error\n                    });\n                }\n            );\n    }\n\n    render() {\n        const { error, isLoaded, items } = this.state;\n        if (error) {\n            return <div>Error: {error.message}</div>;\n        } else if (!isLoaded) {\n            return <div>Loading...</div>;\n        } else {\n            return (\n                <ul>\n                    {items.map((item) => (\n                        <li key={item.id}>\n                            {item.name} {item.price}\n                        </li>\n                    ))}\n                </ul>\n            );\n        }\n    }\n}\n"}],"position":{"start":{"line":35,"column":1,"offset":1332},"end":{"line":86,"column":4,"offset":2602}}}],"position":{"start":{"line":35,"column":1,"offset":1332},"end":{"line":86,"column":4,"offset":2602}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Here is the equivalent with ","position":{"start":{"line":88,"column":1,"offset":2604},"end":{"line":88,"column":29,"offset":2632}}},{"type":"element","tagName":"a","properties":{"href":"https://reactjs.org/docs/hooks-intro.html"},"children":[{"type":"text","value":"Hooks","position":{"start":{"line":88,"column":30,"offset":2633},"end":{"line":88,"column":35,"offset":2638}}}],"position":{"start":{"line":88,"column":29,"offset":2632},"end":{"line":88,"column":79,"offset":2682}}},{"type":"text","value":":","position":{"start":{"line":88,"column":79,"offset":2682},"end":{"line":88,"column":80,"offset":2683}}}],"position":{"start":{"line":88,"column":1,"offset":2604},"end":{"line":88,"column":80,"offset":2683}}},{"type":"text","value":"\n"},{"type":"element","tagName":"hr","properties":{},"children":[],"position":{"start":{"line":90,"column":1,"offset":2685},"end":{"line":90,"column":4,"offset":2688}}},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-js"]},"children":[{"type":"text","value":"//\n\n\nfunction MyComponent() {\n    const [error, setError] = useState(null);\n    const [isLoaded, setIsLoaded] = useState(false);\n    const [items, setItems] = useState([]);\n\n    useEffect(() => {\n        fetch('https://api.example.com/items')\n            .then((res) => res.json())\n            .then(\n                (result) => {\n                    setIsLoaded(true);\n                    setItems(result);\n                },\n\n                (error) => {\n                    setIsLoaded(true);\n                    setError(error);\n                }\n            );\n    }, []);\n\n    if (error) {\n        return <div>Error: {error.message}</div>;\n    } else if (!isLoaded) {\n        return <div>Loading...</div>;\n    } else {\n        return (\n            <ul>\n                {items.map((item) => (\n                    <li key={item.id}>\n                        {item.name} {item.price}\n                    </li>\n                ))}\n            </ul>\n        );\n    }\n}\n"}],"position":{"start":{"line":93,"column":1,"offset":2691},"end":{"line":134,"column":4,"offset":3669}}}],"position":{"start":{"line":93,"column":1,"offset":2691},"end":{"line":134,"column":4,"offset":3669}}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":135,"column":1,"offset":3670}}}}